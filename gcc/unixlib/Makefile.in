all:	unixlib

.PHONY: unixlib riscos-dist

upath=$(objdir)/unixlib/$(TRG)

DIRS = \

unixlib: $(DIRS)
	$(MAKE) -C source

riscos-dist:
	mkdir -p $(info_dir)/UnixLib
	cp -p Docs/Changelog $(info_dir)/UnixLib
	cp -p Docs/Copyright $(info_dir)/UnixLib
	cp -p Docs/ReadMe $(info_dir)/UnixLib

$(DIRS):
	mkdir -p $@
