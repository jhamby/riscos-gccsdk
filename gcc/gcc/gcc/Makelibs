OPTS = -mthrowback -mpoke-function-name
#OPTS = -mthrowback -mno-apcs-frame -mno-apcs-stack-check -muse-arg-pointer

LIBGCC2_CFLAGS = -DIN_GCC -DHAVE_CONFIG_H -DIN_LIBGCC2 -D__GCC_FLOAT_NOT_NEEDED
CC = $(standard_exec_prefix)/gcc -O2 $(OPTS) $(CFL) -I. -I../../include -I./include -I./config -I./cp $(LIBGCC2_CFLAGS)
CC_S = $(standard_exec_prefix)/gcc -xassembler-with-cpp

libgccobjdir=$(objdir)/gcc-$(VERSION)/libgcc/$(TRG)

# All the file that go into the gcc library
GCC_LIB_COMMON = $(libgccobjdir)/dummy.o \
	$(libgccobjdir)/getopt.o \
	$(libgccobjdir)/getopt1.o \
	$(libgccobjdir)/floatdidf.o \
	$(libgccobjdir)/divsi3.o \
	$(libgccobjdir)/modsi3.o \
	$(libgccobjdir)/umodsi3.o \
	$(libgccobjdir)/udivsi3.o \
	$(libgccobjdir)/fixsfdi.o \
	$(libgccobjdir)/fixunsxfdi.o \
	$(libgccobjdir)/fixunsdfdi.o \
	$(libgccobjdir)/fixunssfdi.o \
	$(libgccobjdir)/libgcc_fix.o \
	$(libgccobjdir)/ffs.o \
	$(libgccobjdir)/dynamic.o \
	$(libgccobjdir)/gcc_bcmp.o \
	$(libgccobjdir)/ashldi3.o \
	$(libgccobjdir)/ashrdi3.o \
	$(libgccobjdir)/lshrdi3.o \
	$(libgccobjdir)/clricache.o \
	$(libgccobjdir)/divdi3.o \
	$(libgccobjdir)/muldi3.o \
	$(libgccobjdir)/moddi3.o \
	$(libgccobjdir)/udivdi3.o \
	$(libgccobjdir)/umoddi3.o \
	$(libgccobjdir)/cmpdi2.o \
	$(libgccobjdir)/ucmpdi2.o \
	$(libgccobjdir)/ctors.o \
	$(libgccobjdir)/nextarg.o \
	$(libgccobjdir)/main.o \
	$(libgccobjdir)/eh.o \
	$(libgccobjdir)/eprintf.o \
	$(libgccobjdir)/pure.o \
	$(libgccobjdir)/bb.o \
	$(libgccobjdir)/div0.o

FP_BIT_DOUBLE = $(libgccobjdir)/pack_df.o \
	$(libgccobjdir)/unpack_df.o \
	$(libgccobjdir)/addsub_df.o \
	$(libgccobjdir)/mul_df.o \
	$(libgccobjdir)/div_df.o \
	$(libgccobjdir)/fpcmp_p_df.o \
	$(libgccobjdir)/compare_df.o \
	$(libgccobjdir)/eq_df.o \
	$(libgccobjdir)/ne_df.o \
	$(libgccobjdir)/gt_df.o \
	$(libgccobjdir)/ge_df.o \
	$(libgccobjdir)/lt_df.o \
	$(libgccobjdir)/le_df.o \
	$(libgccobjdir)/si_to_df.o \
	$(libgccobjdir)/df_to_si.o \
	$(libgccobjdir)/df_to_usi.o \
	$(libgccobjdir)/negate_df.o \
	$(libgccobjdir)/make_df.o \
	$(libgccobjdir)/df_to_sf.o

FP_BIT_SINGLE = $(libgccobjdir)/pack_sf.o \
	$(libgccobjdir)/unpack_sf.o \
	$(libgccobjdir)/addsub_sf.o \
	$(libgccobjdir)/mul_sf.o \
	$(libgccobjdir)/div_sf.o \
	$(libgccobjdir)/fpcmp_p_sf.o \
	$(libgccobjdir)/compare_sf.o \
	$(libgccobjdir)/eq_sf.o \
	$(libgccobjdir)/ne_sf.o \
	$(libgccobjdir)/gt_sf.o \
	$(libgccobjdir)/ge_sf.o \
	$(libgccobjdir)/lt_sf.o \
	$(libgccobjdir)/le_sf.o \
	$(libgccobjdir)/si_to_sf.o \
	$(libgccobjdir)/sf_to_usi.o \
	$(libgccobjdir)/sf_to_si.o \
	$(libgccobjdir)/negate_sf.o \
	$(libgccobjdir)/make_sf.o \
	$(libgccobjdir)/sf_to_df.o


GPP_LIB = $(libgccobjdir)/tinfo.o \
	$(libgccobjdir)/tinfo2.o \
	$(libgccobjdir)/new.o \
	$(libgccobjdir)/opnew.o \
	$(libgccobjdir)/opnewnt.o \
	$(libgccobjdir)/opvnew.o \
	$(libgccobjdir)/opvnewnt.o \
	$(libgccobjdir)/opdel.o \
	$(libgccobjdir)/opdelnt.o \
	$(libgccobjdir)/opvdel.o \
	$(libgccobjdir)/opvdelnt.o \
	$(libgccobjdir)/exception.o

all:	$(TBIN)/$(TRG)/libgcc.o

install_headers:
	cp -f ginclude/iso646.h $(gcc_include_dir)
	#cp -f ginclude/proto.h $(gcc_include_dir)
	cp -f ginclude/stdarg.h $(gcc_include_dir)
	cp -f ginclude/varargs.h $(gcc_include_dir)
	#cp ginclude/_G_config.h $(gcc_include_dir)
	#cp ginclude/assert.h $(gcc_include_dir)

$(TBIN)/$(TRG)/libgcc.o: $(GPP_LIB) $(GCC_LIB_ULIB) $(GCC_LIB_COMMON) $(FP_BIT_DOUBLE) $(FP_BIT_SINGLE)
	$(standard_exec_prefix)/libfile -c $@ $(GPP_LIB) $(GCC_LIB_ULIB) $(GCC_LIB_COMMON) $(FP_BIT_DOUBLE) $(FP_BIT_SINGLE)


############################################################################

# GCC standard run-time libraries
$(libgccobjdir)/getopt.o:	../../libiberty/getopt.c
	$(CC) -o $@ -c ../../libiberty/getopt.c

$(libgccobjdir)/getopt1.o:	../../libiberty/getopt1.c
	$(CC) -o $@ -c ../../libiberty/getopt1.c

$(libgccobjdir)/dummy.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL__dummy
$(libgccobjdir)/gcc_bcmp.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL__gcc_bcmp

$(libgccobjdir)/eh.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL_eh -DDONT_USE_BUILTIN_SETJMP
$(libgccobjdir)/eprintf.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL_eprintf
$(libgccobjdir)/pure.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL_pure
$(libgccobjdir)/main.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL__main -DCTOR_LISTS_DEFINED_EXTERNALLY
$(libgccobjdir)/bb.o:	libgcc2.c
	$(CC) -o $@ -c libgcc2.c -DL_bb
$(libgccobjdir)/div0.o:	config/arm/lib1aof.s
	$(CC_S) -c config/arm/lib1aof.s -o $@ -DL_div0

# C++ language-support library pieces for libgcc
$(libgccobjdir)/tinfo.o:	cp/tinfo.cc
	$(CC) -o $@ -c cp/tinfo.cc -I./cp/inc
$(libgccobjdir)/tinfo2.o:	cp/tinfo2.cc
	$(CC) -o $@ -c cp/tinfo2.cc -I./cp/inc
$(libgccobjdir)/exception.o:	cp/exception.cc
	$(CC) -o $@ -c cp/exception.cc -I./cp/inc -fexceptions
$(libgccobjdir)/new.o:	cp/new.cc
	$(CC) -o $@ -c cp/new.cc -I./cp/inc
$(libgccobjdir)/opnew.o:	cp/new1.cc
	$(CC) -o $@ -c cp/new1.cc -I./cp/inc -DL_op_new -fexceptions
$(libgccobjdir)/opnewnt.o:	cp/new1.cc
	$(CC) -o $@ -c cp/new1.cc -I./cp/inc -DL_op_newnt -fexceptions
$(libgccobjdir)/opvnew.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_vnew
$(libgccobjdir)/opvnewnt.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_vnewnt
$(libgccobjdir)/opdel.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_delete
$(libgccobjdir)/opdelnt.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_delnt
$(libgccobjdir)/opvdel.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_vdel
$(libgccobjdir)/opvdelnt.o:	cp/new2.cc
	$(CC) -o $@ -c cp/new2.cc -I./cp/inc -DL_op_vdelnt

# Assembler versions of certain library functions

