# fp-bit library (double precision)
$(libgccobjdir)/pack_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_pack_df
$(libgccobjdir)/unpack_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_unpack_df
$(libgccobjdir)/addsub_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_addsub_df
$(libgccobjdir)/mul_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_mul_df
