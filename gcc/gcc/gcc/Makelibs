	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_negate_df
$(libgccobjdir)/make_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_make_df


# fplib (single precision)
