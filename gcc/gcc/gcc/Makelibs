	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_fpcmp_parts_df
$(libgccobjdir)/compare_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_compare_df
$(libgccobjdir)/eq_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_eq_df
$(libgccobjdir)/ne_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_ne_df
$(libgccobjdir)/gt_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_gt_df
$(libgccobjdir)/ge_df.o:	config/fp-bit.c
	$(CC) -o $@ -c config/fp-bit.c -DFINE_GRAINED_LIBRARIES -DL_ge_df
