AB_INSTALL=yes
AB_CONFLAGS=--enable-compat185

cd dist

# Debian style setup 
ln -sf $GCCSDK_INSTALL_ENV/lib/libdb.a $GCCSDK_INSTALL_ENV/lib/libdb-3.a
ln -sf $GCCSDK_INSTALL_ENV/lib/libdb.a $GCCSDK_INSTALL_ENV/lib/libdb3.a

mkdir -p $D/lib/
ln -sf $D/lib/libdb.a $D/lib/libdb-3.a
ln -sf $D/lib/libdb.a $D/lib/libdb3.a

