AB_INSTALL=yes
AB_ROVERSION=2

ab_package() {
  ab_create_app LibTASN1 Apps/Library libtasn1-dev

  HERE=`pwd`

  cd $A

  unzip $AB_OUTPUTLIB/$RO_PKG.zip

  mv include/h h
  rm -rf include

  mv lib/*.a .
  rm -rf lib

  cd doc
  docdir=`echo *`
  mv $docdir/* .
  rm -rf $docdir
  
  cd ../man
  find . -maxdepth 1 -type f -execdir mv {} {},faf \;

  cd $HERE

  $AB_HOME/add-riscpkg -name LibTASN1-Dev -unixlib -copyrightfile $S/COPYING
}
