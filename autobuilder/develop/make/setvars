
AB_CATEGORY=Develop

AB_PRECONF="CFLAGS=\"-DNO_ARCHIVES -DSYS_SIGLIST_DECLARED -O2\""

ab_package() {

  mkdir -p $D/Apps/$AB_CATEGORY/\!GCC/bin/
  mkdir -p $D/Apps/$AB_CATEGORY/\!GCC/docs/

  cp $S/make$AB_EXEEXT $D/Apps/$AB_CATEGORY\/!GCC/bin/
  rman -f HTML $S/make.1 > $D/Apps/$AB_CATEGORY/\!GCC/docs/make.html,faf

  $AB_HOME/add-riscpkg -unixlib
}
