AB_CATEGORY=App/Develop

AB_PRECONF="CFLAGS=\"-DNO_ARCHIVES -DSYS_SIGLIST_DECLARED -O2\""

ab_package() {
  mkdir -p $D/$AB_CATEGORY/\!GCC/bin/
  mkdir -p $D/$AB_CATEGORY/\!GCC/doc/

  cp $S/make$AB_EXEEXT $D/$AB_CATEGORY\/!GCC/bin/
  rman -f HTML $S/make.1 > $D/$AB_CATEGORY/\!GCC/doc/make.html,faf

  $AB_HOME/add-riscpkg -unixlib
}
