
AB_CATEGORY=Development

ab_package() {

  A=$D/$AB_PACKAGE/Apps/$AB_CATEGORY/\!GCC
  mkdir -p $A/bin/
  mkdir -p $A/docs/


  cp $S/src/bison$AB_EXEEXT $A/bin
  rman -f HTML $S/doc/bison.1 > $A/docs/bison.html,faf

  export AB_SUBPACKAGE=$AB_PACKAGE

  $AB_HOME/add-riscpkg -unixlib

}

touch --date="Jan 01 2000" \
  doc/bison.info \
  doc/bison.texinfo \
  doc/fdl.texi
