
AB_CONFLAGS="\
--with-apr=$GCCSDK_INSTALL_ENV/bin/apr-1-config \
--with-apr-util=$GCCSDK_INSTALL_ENV/bin/apu-1-config \
--with-neon=$GCCSDK_INSTALL_ENV \
--without-berkeley-db \
--without-apxs"

AB_CATEGORY=Develop

ab_package() {
  cp $S/subversion/svn/svn$AB_EXEEXT $S/COPYING $D
}

export LIBS=-lexpat

