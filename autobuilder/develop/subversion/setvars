
AB_CONFLAGS="\
--with-apr=$GCCSDK_INSTALL_ENV/bin/apr-1-config \
--with-apr-util=$GCCSDK_INSTALL_ENV/bin/apu-1-config \
--with-neon=$GCCSDK_INSTALL_ENV \
--without-berkeley-db \
--without-apxs"

AB_CATEGORY=Develop

ab_package() {
  ab_create_command_app Subversion Apps/Develop

  ab_add_commands $S/subversion/svn/svn$AB_EXEEXT
  ab_docs $S/COPYING

  $AB_HOME/add-riscpkg -unixlib
}

export LIBS=-lexpat

