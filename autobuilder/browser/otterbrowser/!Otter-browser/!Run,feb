Wimpslot -min 16384k -max 16384k

| These are only relevant if the JIT is enabled.
Set JSC_useHalfWord false
Set JSC_useBX false
Set JSC_useDoubleWord false

| Sets the language to use for the Spell Checker
Set UnixEnv$LANG "en-GB"

X removeda otter-browser
X rmreinit somanager
X removeda "Qt mmap"
X removeda "Webkit Fast Malloc"
X removeda "Webkit GC Heap"
X removeda "Webkit VM Stack"

Set Otter-browser$Dir <Obey$Dir>
<Otter-browser$Dir>.Poll 3
<Otter-browser$Dir>.Banner
<Otter-browser$Dir>.redblueswap
Run <Otter-browser$Dir>.otter-browser ><Otter-browser$Dir>.debug 2>&1
