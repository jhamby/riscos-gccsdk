

AB_CATEGORY=cli/test

AB_CONFLAGS="--without-bash-malloc --disable-process-substitution"
AB_MAKE=make

ln -s mksignames,ff8 mksignames
ln -s mksyntax,ff8 mksyntax

AB_MANPAGE=doc/bash.1

touch configure.in aclocal.m4 stamp-h.in Makefile.in config.status

ab_package () {
  cp -av $S/bash,ff8 $S/COPYING $D
}

