AB_CATEGORY=Command

rm -f crc/*.o crc/*.a
make -C crc
rm -f crc/*.o crc/*.a

AB_MANPAGE="man/binhex.1 man/frommac.1 man/hexbin.1 man/macsave.1 man/macstream.1 man/macunpack.1 man/macutil.1 man/tomac.1"

ab_package() {
  cp $S/binhex/binhex$AB_EXEEXT $D
  cp $S/comm/tomac$AB_EXEEXT $D
  cp $S/comm/frommac$AB_EXEEXT $D
  cp $S/mixed/macsave$AB_EXEEXT $D
  cp $S/mixed/macstream$AB_EXEEXT $D
  cp $S/hexbin/hexbin$AB_EXEEXT $D
  cp $S/macunpack/macunpack$AB_EXEEXT $D
}
