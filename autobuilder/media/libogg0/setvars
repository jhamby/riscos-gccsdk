AB_INSTALL=yes

ab_package() {
  ab_create_app LibOGG Apps/Library libogg-dev

  HERE=`pwd`

  cd $A

  unzip $AB_OUTPUTLIB/$RO_PKG.zip

  mv include/ogg ogg
  rm -rf include

  mv lib/*.a .
  rm -rf lib

  cd doc
  docdir=`echo *`
  mv $docdir/* .
  rm -rf $docdir

  cd $HERE

  # Copy HTML documentation
  cp -r $S/doc $A

  $AB_HOME/add-riscpkg -unixlib -unixsound
}
