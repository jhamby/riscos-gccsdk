AB_NOREMOTE=yes

AB_PVERSION=1.00
AB_ROVERSION=1
AB_CATEGORY=Admin

AB_PRECONF=:

AB_COMPILEFAIL=yes

AB_INSTALL=no
AB_CONFIG=:

AB_MAKE=:

ab_package () {

   mkdir -p $D/RaspberryPiDistro/
   $AB_HOME/add-riscpkg -section Admin -package RaspberryPiDistro       # Last step to complete packaging
 }
